class Solution {
    public int searchInsert(int[] nums, int target) {
        int p=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]==target){ 
                p=i;
            }
        }
        if(p==0){
            for(int j=0;j<nums.length;j++){
                if(nums[j]<target){
                    p=j+1;
                }
            }
        }
        return p;
    }
}
